[user]
	name = Antoine Busque
	email = abusque@efficios.com
[color]
	ui = auto
	ui = true
[push]
	default = current
[alias]
	ci = commit
        cim = commit -m
        cam = commit -am
        co = checkout
        st = status
        lpg = log --pretty=oneline --graph
        lppg = log --graph --pretty=format:'%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
        discard = stash --keep-index -u
        k = !gitk
        d = diff
        ds = diff --staged
        changelog = log --pretty='%x09* %s'
[sendemail]
	smtpencryption = tls
	smtpserver = mail.efficios.com
	smtpuser = antoine.busque@efficios.com
	smtpserverport = 587
	smtppass = <PASS>
	suppresscc = self
[merge]
	conflictstyle = diff3
